  "test_scenarios": [
    {
      "scenario": "Critical Emergency",
      "description": "Patient with multiple life-threatening vital signs",
      "input": {
        "resp_rate": 35,
        "spo2": 85,
        "bp_sys": 200,
        "pulse": 150,
        "temp": 40.0,
        "consciousness": "Unresponsive"
      },
      "expected_output": "MER Call",
      "priority": "Critical"
    },
    {
      "scenario": "Moderate Concern",
      "description": "Patient with some abnormal but not critical vitals",
      "input": {
        "resp_rate": 25,
        "spo2": 94,
        "bp_sys": 150,
        "pulse": 100,
        "temp": 37.5,
        "consciousness": "Alert"
      },
      "expected_output": "MDT Review",
      "priority": "Medium"
    },
    {
      "scenario": "Single Abnormality",
      "description": "Patient with one concerning vital sign",
      "input": {
        "resp_rate": 20,
        "spo2": 98,
        "bp_sys": 170,
        "pulse": 80,
        "temp": 37.0,
        "consciousness": "Alert"
      },
      "expected_output": "RN Review",
      "priority": "Low"
    },
    {
      "scenario": "Normal Patient",
      "description": "Healthy patient with all normal vitals",
